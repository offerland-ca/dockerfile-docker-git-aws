{
  "group": {
    "default": {
      "targets": [
        "internet"
      ]
    }
  },
  "variable": {
    "TAG": {
      "default": "latest"
    }
  },
  "target": {
    "internet": {
      "dockerfile": "Dockerfile",
      "args": {
        "base_image": "docker:${TAG}"
      },
      "tags": [
        "ghcr.io/offerland-canada/dockerfile-docker-git-aws:${TAG}"
      ]
    }
  }
}
